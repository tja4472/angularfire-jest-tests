{
  "name": "ng-jest-cypress-template",
  "version": "0.0.0",
  "scripts": {
    "--- emulators ---": "",
    "emulators:start-demo": "firebase emulators:start --only firestore,auth --project demo-1",
    "--- start ---": "",
    "start": "ng serve",
    "-- test ---": "",
    "test": "jest --runInBand --detectOpenHandles",
    "test:verbose": "jest --runInBand --verbose",
    "test:all-emulator": "jest --runInBand --group=emulator-required",
    "test:all-emulator:jsdom": "jest --runInBand --group=emulator-required/jsdom",
    "test:all-emulator:node": "jest --runInBand --group=emulator-required/node",
    "test:all-no-emulator": "jest --group=-emulator-required",
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.2.2",
    "@angular/common": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/core": "13.2.2",
    "@angular/fire": "7.2.1",
    "@angular/forms": "13.2.2",
    "@angular/platform-browser": "13.2.2",
    "@angular/platform-browser-dynamic": "13.2.2",
    "@angular/router": "13.2.2",
    "firebase": "9.6.7",
    "rxfire": "6.0.3",
    "rxjs": "7.5.4",
    "tslib": "2.3.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.3",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.2.3",
    "@angular/compiler-cli": "13.2.2",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.12",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "cypress": "9.1.1",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.3.0",
    "firebase-admin": "10.0.2",
    "firebase-tools": "10.2.2",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.1",
    "jest-runner-groups": "2.1.0",
    "prettier": "2.5.1",
    "ts-snippet": "5.0.2",
    "typescript": "4.5.5"
  }
}