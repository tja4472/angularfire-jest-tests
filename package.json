{
  "name": "ng-jest-cypress-template",
  "version": "0.0.0",
  "scripts": {
    "--- emulators ---": "",
    "emulators:start-demo": "firebase emulators:start --only firestore,auth --project demo-1",
    "--- start ---": "",
    "start": "ng serve",
    "-- test ---": "",
    "test": "jest --runInBand",
    "test:verbose": "jest --verbose",  
    "test:all-emulator": "jest --runInBand --group=emulator-required",
    "test:all-emulator:jsdom": "jest --runInBand --group=emulator-required/jsdom",  
    "test:all-emulator:node": "jest --runInBand --group=emulator-required/node",       
    "test:all-no-emulator": "jest --group=-emulator-required",      
    "ng": "ng",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.12",
    "@angular/common": "12.2.12",
    "@angular/compiler": "12.2.12",
    "@angular/core": "12.2.12",
    "@angular/fire": "7.1.1",
    "@angular/forms": "12.2.12",
    "@angular/platform-browser": "12.2.12",
    "@angular/platform-browser-dynamic": "12.2.12",
    "@angular/router": "12.2.12",
    "firebase": "9.4.1",
    "rxjs": "6.6.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.12",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "12.2.12",
    "@angular/compiler-cli": "12.2.12",
    "@types/jest": "27.0.2",
    "@types/node": "12.20.27",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "cypress": "8.7.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "firebase-admin": "10.0.0",    
    "firebase-tools": "9.22.0",
    "jest": "27.3.1",
    "jest-preset-angular": "10.1.0",
    "jest-runner-groups": "2.1.0",    
    "prettier": "2.4.1",
    "ts-snippet": "5.0.2",    
    "typescript": "4.3.5"
  }
}
